{
  "builds": [
    {
      "src": "index.js",
      "use": "@vercel/node"
    },
    {
      "src": "build/**",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/tasks",
      "dest": "index.js"
    },
    {
      "src": "/tasks/(.*)",
      "dest": "index.js"
    },
    {
      "src": "/auth",
      "dest": "index.js"
    },
    {
      "src": "/auth/(.*)",
      "dest": "index.js"
    },
    {
      "src": "/",
      "dest": "build/index.html"
    },
    {
      "src": "/login",
      "dest": "build/index.html"
    },
    {
      "src": "/about",
      "dest": "build/index.html"
    },
    {
      "src": "/contact",
      "dest": "build/index.html"
    },
    {
      "src": "/userinfo",
      "dest": "build/index.html"
    },
    {
      "src": "/(.+)",
      "dest": "build/$1"
    }
  ]
}
